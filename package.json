    "svg",
    "data",
    "store",
    "collection",
    "bus",
    "event",
    "bus",
    "NGN",
    "ngn",
    "orchestration"
  ],
  "author": {
    "name": "Corey Butler",
    "url": "http://coreybutler.com"
  },
  "contributors": [
    {
      "name": "Graham Butler",
      "url": "http://grahambutler.com"
    }
  ],
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "http://ngn.js.org",
  "devDependencies": {
    "babel-preset-es2015": "6.9.0",
