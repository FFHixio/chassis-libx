{
  "name": "ngnx-chassis",
  "version": "1.7.11",
  "description": "The opininated 'extra' library for NGN.",
  "main": "index.js",
  "scripts": {
    "test": "karma start karma.conf.js && standard --verbose | snazzy",
    "gutcheck": "BUILD_MODE=live karma start karma.conf.js && standard --verbose | snazzy",
    "fulltest": "BUILD_MODE=prod karma start karma.conf.js && standard --verbose | snazzy",
    "firefox": "karma start karma.conf.js --firefox && standard --verbose | snazzy",
    "safari": "karma start karma.conf.js --safari && standard --verbose | snazzy",
    "edge": "karma start karma.conf.js --edge && standard --verbose | snazzy",
    "ie": "karma start karma.conf.js --ie && standard --verbose | snazzy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngnjs/chassis-lib.git"
  },
  "keywords": [
    "chassis",
    "chassisx",
    "ngnx",
    "NGNX",
    "js",
    "dom",
    "http",
    "ajax",
    "http",
    "references",
    "ref",
    "svg",
    "data",
    "store",
    "collection",
    "bus",
    "event",
    "bus",
    "NGN",
    "ngn",
    "orchestration"
  ],
  "author": {
    "name": "Corey Butler",
    "url": "http://coreybutler.com"
  },
  "contributors": [
    {
      "name": "Graham Butler",
      "url": "http://grahambutler.com"
    }
  ],
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "http://ngn.js.org",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "browserify": "^14.4.0",
    "buffer-equal-constant-time": "1.0.1",
    "caniuse-api": "^2.0.0",
    "colors": "^1.1.2",
    "del": "^2.2.2",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-footer": "^1.0.5",
    "gulp-header": "^1.8.8",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.1.1",
    "karma-edge-launcher": "^0.4.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-spec-reporter": "0.0.31",
    "karma-tap": "^3.1.1",
    "localenvironment": "^1.0.2",
    "musthave": "^1.0.2",
    "phantomjs-prebuilt": "^2.1.14",
    "publish-release": "^1.3.3",
    "pump": "^1.0.2",
    "shortbus": "^1.2.7",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "tape": "^4.6.3",
    "through2": "^2.0.3",
    "watchify": "^3.9.0"
  },
  "standard": {
    "ignore": [
      "_*",
      "_**/*",
      "dist/**/*",
